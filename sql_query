\cocoatextscaling0\cocoaplatform0{\fonttbl\f0\fswiss\fcharset0 Helvetica;\f1\fswiss\fcharset0 Helvetica-Bold;}
{\colortbl;\red255\green255\blue255;}
{\*\expandedcolortbl;;}
\margl1440\margr1440\vieww12240\viewh9220\viewkind0
\pard\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\tx8640\pardirnatural\partightenfactor0

\f0\fs26 \cf0 \

\f1\b /* Query 1 */\

\f0\b0 SELECT\
  E.FirstName,\
  E.LastName,\
  E.EmployeeId,\
  SUM(I.total) AS Total,\
  E.Title\
FROM Employee AS E\
JOIN Customer AS C\
  ON E.EmployeeId = C.SupportRepId\
JOIN Invoice AS I\
  ON C.CustomerId = I.CustomerId\
GROUP BY E.EmployeeId\
ORDER BY Total DESC;\
\

\f1\b /* Query 2 */\

\f0\b0 SELECT\
  E.FirstName,\
  E.LastName,\
  E.EmployeeId,\
  SUM(I.total) AS Total,\
  C.Country,\
  E.Title\
FROM Employee AS E\
JOIN Customer AS C\
  ON E.EmployeeId = C.SupportRepId\
JOIN Invoice AS I\
  ON C.CustomerId = I.CustomerId\
WHERE E.EmployeeId = 3\
GROUP BY C.Country\
ORDER BY Total DESC;\
\

\f1\b /* Query 3 */\

\f0\b0 SELECT\
  E.FirstName,\
  E.LastName,\
  E.EmployeeId,\
  C.Country,\
  T.Composer,\
  COUNT(*) AS Count\
FROM Employee AS E\
JOIN Customer AS C\
  ON E.EmployeeId = C.SupportRepId\
JOIN Invoice AS I\
  ON C.CustomerId = I.CustomerId\
JOIN InvoiceLine AS IL\
  ON I.InvoiceId = IL.InvoiceId\
JOIN Track AS T\
  ON IL.TrackId = T.TrackId\
JOIN Genre AS G\
  ON T.GenreId = T.GenreId\
WHERE E.EmployeeId = 3\
AND C.Country = 'Canada'\
GROUP BY T.Composer\
ORDER BY Count DESC;\
\

\f1\b /* Query 4 */\

\f0\b0 SELECT\
  E.FirstName,\
  E.LastName,\
  E.EmployeeId,\
  C.Country,\
  C.City,\
  SUM(IL.UnitPrice * IL.Quantity) AS Total\
FROM Employee AS E\
JOIN Customer AS C\
  ON E.EmployeeId = C.SupportRepId\
JOIN Invoice AS I\
  ON C.CustomerId = I.CustomerId\
JOIN InvoiceLine AS IL\
  ON I.InvoiceId = IL.InvoiceId\
JOIN Track AS T\
  ON IL.TrackId = T.TrackId\
JOIN Genre AS G\
  ON T.GenreId = T.GenreId\
WHERE E.EmployeeId = 3\
AND C.Country = 'Canada'\
GROUP BY C.City\
ORDER BY Total DESC;\
}
